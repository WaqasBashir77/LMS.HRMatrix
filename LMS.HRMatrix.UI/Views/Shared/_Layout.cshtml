        .asd {
            width: 100%;
            overflow-y: auto;
        }

        .media-annotation {
            display: none;
        }

        #chatbox-layer {
            width: auto;
            position: fixed;
            bottom: 33px;
            right: 0;
            padding-bottom: 0;
            z-index: 10000;
        }

        .chatbox, ul li {
            margin-top: 5px !important;
        }

        .message-bubble {
            position: fixed;
            right: 5px;
            bottom: 60px;
        }

            .message-bubble button {
                float: right;
            }

                .message-bubble button span {
                    font-size: large;
                }

        .message-usres-list, ul li {
            padding: 3px 0;
        }

            .message-usres-list li:hover {
                background: #fcf9f9;
                cursor: pointer;
            }

        .chatheads-list {
            height: 300px;
            overflow-x: hidden;
            padding-left: 0;
            margin-left: 0;
        }

            .chatheads-list li {
                list-style: none;
                height: auto;
                clear: both;
                display: -webkit-box;
                font-weight: bold;
            }

                .chatheads-list li:hover {
                    background: #f3f1d0;
                    cursor: pointer;
                }

                .chatheads-list li:after {
                    content: "\A";
                    white-space: pre;
                }

        .chathead {
            height: auto;
        }

            .chathead:hover {
            }

        .chathead-image {
            float: left;
            height: auto;
        }

        .chathead-text {
            float: left;
            height: auto;
            text-transform: uppercase;
        }

        .message {
            color: #808080;
            font-weight: normal;
        }
    </style>
